page {
  background: #fff;
  padding: 0 0rpx 0;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  background-size: cover;
}
/* pages/leftSwiperDel/index.wxss */
view{
    box-sizing: border-box;
}
.background {
    width: 100%;
    height: 100%;
    position:fixed; 
    background-size:100% 100%;
    z-index: -1;
    }
.item-box{
    width: 700rpx;
    height: 10rpx;
    margin: 0 auto;
    padding:40rpx 0;
}
.title{
padding-top: 15px;
  font-size: 20px;
  height: 12px;
  text-align: center;  
}
.items{
    padding-top: 20px;
    width: 100%;
}
.item{
    position: relative;
    border-top: 2rpx solid #eee;
    height: 100rpx;
    line-height: 100rpx;
    overflow: hidden;
     
}
 
 
.item:last-child{
    border-bottom: 2rpx solid #eee;
}
.inner{
    position: absolute;
    top:0;
}
.inner.txt{
    background-color: #fff;
    width: 100%;
    z-index: 5;
    padding:0 10rpx;
    transition: left 0.2s ease-in-out;
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
}

.item-icon{
    width: 64rpx;
    height: 64rpx;
    vertical-align: middle;
    margin-right: 16rpx;
    margin-left:13px;
    border-radius:50%;
 
}
 
.item-data{
  float: right;
  margin-right:5%;
}
.sno{
    color: #87CEFA;
  }
.times{
  color: #87CEFA;
}
navigator{
    right: 40px;
    bottom: 50px;   
    width: 100px;
    background-color: #87CEFA;
    height: 80px;
    padding-top: 28px;
    color: white;
    background-size: cover;
    position: fixed;
    z-index: 9999;
    font-size: 20px;
    text-align: center;
    border: 0 solid #ffffff;
    box-shadow: 4px 1px 1px #cccccc;
}